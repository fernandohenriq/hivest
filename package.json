    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "prettier": "^3.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "reflect-metadata": "^0.1.0",
    "tsyringe": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
